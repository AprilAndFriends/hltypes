SDKROOT = iphoneos;
GCC_SYMBOLS_PRIVATE_EXTERN = YES; // symbols hidden by default
GCC_INLINES_ARE_PRIVATE_EXTERN = YES;
CLANG_CXX_LIBRARY = libc++;
IPHONEOS_DEPLOYMENT_TARGET = 9.0;
COMPRESS_PNG_FILES = NO;
ARCHS = $(ARCHS_STANDARD);
//ARCHS = $(ARCHS_STANDARD_32_BIT);
GCC_C_LANGUAGE_STANDARD = gnu99;
STRINGS_FILE_OUTPUT_ENCODING = UTF-8;
ENABLE_BITCODE = NO;
DEBUG_INFORMATION_FORMAT = dwarf; // embedd debug info in the static libs
CLANG_ENABLE_CODE_COVERAGE = NO; // temp for now, until all unit tests for all april & friends are done