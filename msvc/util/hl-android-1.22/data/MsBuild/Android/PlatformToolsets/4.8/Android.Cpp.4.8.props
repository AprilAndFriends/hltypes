    <PlatformToolsetObjcopyTool>$(TargetPlatformArchitectureToolchainToolPrefix)-objcopy.exe</PlatformToolsetObjcopyTool>
    <PlatformToolsetCompileToolPath>$(PlatformToolsetPrebuilt)\bin</PlatformToolsetCompileToolPath>
    <PlatformToolsetLibToolPath>$(PlatformToolsetPrebuilt)\bin</PlatformToolsetLibToolPath>
