<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <!--
    Import properties for Visual Studio 2012/2013 support. Both use a 'Platform.pros' to an load appropriate toolset .props.
  -->

  <PropertyGroup>
    <RequiresPlatformDefinition Condition="Exists('$(VCTargetsPath)\Microsoft.Cpp.Platform.targets')">true</RequiresPlatformDefinition>
  </PropertyGroup>

  <Import Condition="'$(RequiresPlatformDefinition)' == 'true'" Project="$(VCTargetsPath)\Microsoft.Cpp.Platform.props" />

  <!--
    Import targets for Visual Studio 2010 traditional support.
  -->

  <Import Condition="'$(RequiresPlatformDefinition)' != 'true' and Exists('$(VCTargetsPath)\Platforms\$(Platform)\ImportBefore')" Project="$(VCTargetsPath)\Platforms\$(Platform)\ImportBefore\*.props" />
  <Import Condition="'$(RequiresPlatformDefinition)' != 'true'" Project="$(VCTargetsPath)\Platforms\$(Platform)\Android.Cpp.props" />
  <Import Condition="'$(RequiresPlatformDefinition)' != 'true' and Exists('$(VCTargetsPath)\Platforms\$(Platform)\ImportAfter')" Project="$(VCTargetsPath)\Platforms\$(Platform)\ImportAfter\*.props" />

</Project>

