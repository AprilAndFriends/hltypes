<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>

    <PlatformArchitecture>32</PlatformArchitecture>
    <PlatformToolset Condition="'$(PlatformToolset)' == ''">clang</PlatformToolset>
    <DefaultPlatformToolset Condition="'$(DefaultPlatformToolset)' == ''">$(PlatformToolset)</DefaultPlatformToolset>
    <TargetArchitecture>x86</TargetArchitecture>
    <STLSupport Condition="'$(STLSupport)' == ''">c++_static</STLSupport>

    <!--
      Explictly disable MSBuild's fast up-to-date functionality. 
      
      With this enabled, MSBuild checks for the presence of a '<proj>.lastbuildstate' file - instead of evaluating dependency trees.
      
      TODO: Disabled for now, needs more dev effort to properly ensure all scripts play happy with it set.
    -->

    <!-- <DisableFastUpToDateCheck>true</DisableFastUpToDateCheck> -->

    <!--
      Default intermediate and output directories.
    -->

    <IntDir Condition="'$(IntDir)'=='' and '$(IntermediateOutputPath)'!=''">$(IntermediateOutputPath)</IntDir>
    <IntDir Condition="'$(IntDir)'=='' and '$(IntermediateOutputPath)'==''">$(Platform)\$(Configuration)\</IntDir>
    <OutDir Condition="'$(OutDir)'=='' and '$(SolutionDir)' != ''">$(SolutionDir)$(Platform)\$(Configuration)\</OutDir>
    <OutDir Condition="'$(OutDir)'=='' and '$(SolutionDir)' == ''">$(IntDir)</OutDir>

  </PropertyGroup>

  <Import Project="$(VCTargetsPath)\Platforms\$(Platform)\Android.Paths.default.props" />
  <Import Project="$(VCTargetsPath)\Platforms\$(Platform)\Android.AndroidSdk.default.props" />
  <Import Project="$(VCTargetsPath)\Platforms\$(Platform)\Android.AndroidNdk.default.props" />

</Project>

